{
    "ServiceBus": {
        "Provider": "inmemory",
        "InMemory": {
            "MessengerReference": "WeakReference",
            "MultipleSubscriberInstance": false
        },
        "RabbitMq": {
            "Connection": "amqp://guest:guest@127.0.0.1:5672",
            "ExchangeName": "nerosoft.starfish.exchange",
            "ExchangeType": "fanout",
            "QueueName": "nerosoft.starfish.queue",
            "TopicName": "nerosoft.starfish.topic"
        }
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*"
}
